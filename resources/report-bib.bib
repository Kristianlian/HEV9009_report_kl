@article{janssen_skeletal_2000,
	title = {Skeletal muscle mass and distribution in 468 men and women aged 18–88 yr},
	volume = {89},
	issn = {8750-7587, 1522-1601},
	url = {https://www.physiology.org/doi/10.1152/jappl.2000.89.1.81},
	doi = {10.1152/jappl.2000.89.1.81},
	abstract = {We employed a whole body magnetic resonance imaging protocol to examine the influence of age, gender, body weight, and height on skeletal muscle (SM) mass and distribution in a large and heterogeneous sample of 468 men and women. Men had significantly ( P {\textless} 0.001) more SM in comparison to women in both absolute terms (33.0 vs. 21.0 kg) and relative to body mass (38.4 vs. 30.6\%). The gender differences were greater in the upper (40\%) than lower (33\%) body ( P {\textless} 0.01). We observed a reduction in relative SM mass starting in the third decade; however, a noticeable decrease in absolute SM mass was not observed until the end of the fifth decade. This decrease was primarily attributed to a decrease in lower body SM. Weight and height explained ∼50\% of the variance in SM mass in men and women. Although a linear relationship existed between SM and height, the relationship between SM and body weight was curvilinear because the contribution of SM to weight gain decreased with increasing body weight. These findings indicate that men have more SM than women and that these gender differences are greater in the upper body. Independent of gender, aging is associated with a decrease in SM mass that is explained, in large measure, by a decrease in lower body SM occurring after the fifth decade.},
	language = {en},
	number = {1},
	urldate = {2025-02-10},
	journal = {Journal of Applied Physiology},
	author = {Janssen, Ian and Heymsfield, Steven B. and Wang, ZiMian and Ross, Robert},
	month = jul,
	year = {2000},
	pages = {81--88},
}

@article{hamarsland_native_2019,
	title = {Native {Whey} {Induces} {Similar} {Adaptation} to {Strength} {Training} as {Milk}, despite {Higher} {Levels} of {Leucine}, in {Elderly} {Individuals}},
	volume = {11},
	copyright = {https://creativecommons.org/licenses/by/4.0/},
	issn = {2072-6643},
	url = {https://www.mdpi.com/2072-6643/11/9/2094},
	doi = {10.3390/nu11092094},
	abstract = {Background: Large amounts of protein (40 g) or supplementing suboptimal servings of protein with leucine are able to overcome the anabolic resistance in elderly muscle. Our aim was to compare the effects of supplementation of native whey, high in leucine, with milk on gains in muscle mass and strength during a period of strength training, in elderly individuals. Methods: In this double-blinded, randomized, controlled study, a total of 30 healthy men and women received two daily servings of 20 g of either milk protein or native whey, during an 11-week strength training intervention. Muscle strength, lean mass, m. vastus lateralis thickness, muscle fiber area, and resting and post-exercise phosphorylation of p70S6K, 4E-BP1, and eEF-2 were assessed prior to and after the intervention period. Results: Muscle mass and strength increased, by all measures applied in both groups (p {\textless} 0.001), with no differences between groups (p {\textgreater} 0.25). p70S6K phosphorylation increased ({\textasciitilde}1000\%, p {\textless} 0.045) 2 h after exercise in the untrained and trained state, with no differences between supplements. Total and phosphorylated mTORC-1 decreased after training. Conclusion: Supplementation with milk or native whey during an 11-week strength training period increased muscle mass and strength similarly in healthy elderly individuals.},
	language = {en},
	number = {9},
	urldate = {2025-02-10},
	journal = {Nutrients},
	author = {Hamarsland, Håvard and Johansen, Mathias K. and Seeberg, Fridtjof and Brochmann, Marie and Garthe, Ina and Benestad, Haakon B. and Raastad, Truls},
	month = sep,
	year = {2019},
	pages = {2094},
}


	journal = {American Journal of Physiology-Endocrinology and Metabolism},
	author = {Mori, Takahiro and Ato, Satoru and Knudsen, Jonas R. and Henriquez-Olguin, Carlos and Li, Zhencheng and Wakabayashi, Koki and Suginohara, Takeshi and Higashida, Kazuhiko and Tamura, Yuki and Nakazato, Koichi and Jensen, Thomas E. and Ogasawara, Riki},
	month = oct,

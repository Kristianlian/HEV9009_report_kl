	author = {Figueiredo, Vandré C. and Wen, Yuan and Alkner, Björn and Fernandez-Gonzalo, Rodrigo and Norrbom, Jessica and Vechetti, Ivan J. and Valentino, Taylor and Mobley, C. Brooks and Zentner, Gabriel E. and Peterson, Charlotte A. and McCarthy, John J. and Murach, Kevin A. and Von Walden, Ferdinand},
	month = dec,
	year = {2020},
	doi = {10.1101/2020.12.14.422642},
	file = {Versjon  sendt inn:C\:\\Users\\krist\\Zotero\\storage\\VCZQPBVH\\Figueiredo et al. - 2020 - Genetic and Epigenetic Regulation of Skeletal Musc.pdf:application/pdf},
}

